<?xml version="1.0"?>
<robot name="diffdrive">
    <link name="world" />
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.6 0.3 0.1"/>
            </geometry>
            <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
        </visual>
    </link>
    <joint name="joint_name" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="world"/>
        <child link="base_link"/>
    </joint>
    <link name="front_left_wheel">
        <visual>
            <geometry>
                <cylinder length="0.05" radius="0.1"/>
            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="1.57079633 0.0 0.0"/>
        </visual>
    </link>
    <joint name="front_left_wheel_to_base_link" type="continuous">
        <parent link="base_link"/>
        <child link="front_left_wheel"/>
        <axis xyz="0 1 0"/>
        <origin xyz="0.15 0.175 0.1"/>
    </joint>
    <link name="front_right_wheel">
        <visual>
            <geometry>
                <cylinder length="0.05" radius="0.1"/>
            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="1.57079633 0.0 0.0"/>
        </visual>
    </link>
    <joint name="front_right_wheel_to_base_link" type="continuous">
        <parent link="base_link"/>
        <child link="front_right_wheel"/>
        <axis xyz="0 1 0"/>
        <origin xyz="0.15 -0.175 0.1"/>
    </joint>
    <link name="back_left_wheel">
        <visual>
            <geometry>
                <cylinder length="0.05" radius="0.1"/>
            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="1.57079633 0.0 0.0"/>
        </visual>
    </link>
    <joint name="back_left_wheel_to_base_link" type="continuous">
        <parent link="base_link"/>
        <child link="back_left_wheel"/>
        <axis xyz="0 1 0"/>
        <origin xyz="-0.15 0.175 0.1"/>
    </joint>
    <link name="back_right_wheel">
        <visual>
            <geometry>
                <cylinder length="0.05" radius="0.1"/>
            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="1.57079633 0.0 0.0"/>
        </visual>
    </link>
    <joint name="back_right_wheel_to_base_link" type="continuous">
        <parent link="base_link"/>
        <child link="back_right_wheel"/>
        <axis xyz="0 1 0"/>
        <origin xyz="-0.15 -0.175 0.1"/>
    </joint>
</robot>